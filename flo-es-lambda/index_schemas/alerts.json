{
    "settings":{
    "number_of_shards":3,
    "number_of_replicas":1
    },
     
    "mappings":{
        "alert":{
            "properties":{
                "icd_id":{"type":"keyword"},
                "incident_id":{"type":"keyword"},
                "alarm_id":{"type":"integer"},
                "alarm_id_system_mode": {"type": "keyword"},
                "system_mode":{"type":"integer"},

                "incident_time":{"type":"date"},
                "updated_at":{"type":"date"},

                "severity":{"type":"integer"},
                "status":{"type":"integer"},

                "friendly_description": {"type":"text"},
                "friendly_name":{"type":"text"},
                "extra_info":{"type":"text"},

                "acknowledged_by_user":{"type":"integer"},
                "user_action_taken": {
                    "type": "object",
                    "properties": {
                        "action_id": {"type":"integer"},
                        "user_id": {"type":"keyword"},
                        "app_used": {"type":"integer"},
                        "execution_time": {"type":"date"}
                    }
                },

                "self_resolved": {"type":"integer"},
                "self_resolved_message": {
                    "type": "keyword"
                },

                "is_cleared": {"type": "boolean"},

               "telemetry_data":{
                    "type":"object",
                    "properties": {
                        "f":{"type":"float"},
                        "fd":{"type":"integer"},   
                        "fdl":{"type":"integer"},
                        "ft":{"type":"float"},
                        "ftl":{"type":"float"},
                        "m":{"type":"integer"},
                        "mafr":{"type":"float"},
                        "o":{"type":"integer"},
                        "p":{"type":"float"},
                        "pef":{"type":"float"},
                        "pefl":{"type":"float"},
                        "pmax":{"type":"float"},
                        "pmin":{"type":"float"},
                        "sw1":{"type":"integer"},
                        "sw2":{"type":"integer"},
                        "t":{"type":"float"},
                        "tmax":{"type":"float"},
                        "tmin":{"type":"float"},
                        "wf":{"type":"float"}
                    }
                },

                "default_delivery_media":{"type":"integer"},

                "user_delivery_media": {
                  "type": "nested",
                  "properties": {
                    "user_id": {"type":"keyword"},
                    "delivery_media": {"type":"integer"}
                  }
                },

                "user_actions": {
                  "type":"object",
                  "properties": {
                      "timeout": {"type": "integer"},
                      "actions": {
                        "type": "nested",
                        "properties": {
                          "display": {
                            "type": "keyword"
                          },
                          "id": {"type": "integer"},
                          "option": {
                            "type": "keyword"
                          },
                          "sort": {"type": "integer"},
                          "display_when": {
                            "type": "object",
                            "properties": {
                              "valve": {
                                "type": "object",
                                "properties": {
                                  "state": {"type": "integer"}
                                }
                              },
                              "alarm_notification_delivery_filter": {
                                "type": "object",
                                "properties": {
                                  "status": {"type": "integer"}
                                }
                              },
                              "time": {
                                "type": "object",
                                "properties": {
                                  "chronometer_type": {"type": "integer"},
                                  "time_elapsed_in_minutes": {"type": "integer"}
                                }
                              },
                              "chains": {
                                "type": "object",
                                "properties": {
                                  "actions_id": {"type": "integer"}
                                }
                              }
                            }
                          },
                          "success_message": {"type":"text"},
                          "failure_message": {"type":"text"}
                        }
                      },
                      "prompt": {"type":"text"},
                      "title": {"type":"text"},
                      "support_options": {
                        "type":"nested",
                        "properties": {
                          "text": {"type":"text"},
                          "action_type":{"type":"integer"},
                          "action_path":{"type":"text"},
                          "id":{"type":"integer"},
                          "sort":{"type":"integer"}
                        }
                      }
                    }
                },
                "account": {
                      "type":"object",
                      "properties": {
                          "account_id": {"type":"keyword"},
                          "group_id": {"type":"keyword"}
                      }
                  },
                 
                "geo_location":{
                    "type":"object",
                    "properties":{
                        "location_id": {"type":"keyword"},
                        "country": {"type":"keyword"},
                        "state_or_province": {"type":"keyword"},
                        "city": {"type":"keyword"},
                        "postal_code": {"type":"keyword"},
                        "timezone": {"type":"keyword"}
                    }
                },

                "onboarding": {
                  "type": "object",
                  "properties": {
                    "event": {"type": "float"},
                    "created_at": {"type":"date"}
                  }
                }
            }
        }
    }
}
