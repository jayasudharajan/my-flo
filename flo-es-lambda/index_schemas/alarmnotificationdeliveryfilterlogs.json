{
    "settings":{
        "number_of_shards":3,
        "number_of_replicas":1
    },
     
    "mappings":{
        "alarmnotificationdeliveryfilterlog":{
                "properties":{
                "alarm_id":{"type":"integer"},
                "created_at":{"type":"date"},
                "expires_at":{"type":"date"},
                "icd_id":{"type":"keyword"},
                "incident_time":{"type":"date"},
                "last_decision_user_id":{"type":"keyword"},
                "last_icd_alarm_incident_registry_id":{"type":"keyword"},
                "severity":{"type":"integer"},
                "status":{"type":"integer"},
                "system_mode":{"type":"integer"},
                "updated_at":{"type":"date"},
 
 
                "account": {
                    "type":"object",
                    "properties": {
                        "account_id": {"type":"keyword"},
                        "group_id": {"type":"keyword"}
                    }
                },
                 
                "geo_location":{
                    "type":"object",
                    "properties":{
                        "location_id": {"type":"keyword"},
                        "country": {"type":"keyword"},
                        "state_or_province": {"type":"keyword"},
                        "city": {"type":"keyword"},
                        "postal_code": {"type":"keyword"},
                        "timezone": {"type":"keyword"}
                    }
                },

                "onboarding": {
                  "type": "object",
                  "properties": {
                    "event": {"type": "float"},
                    "created_at": {"type":"date"}
                  }
                }
            }
        }
    }
}