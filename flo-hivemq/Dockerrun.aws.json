{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "local-docker",
      "host": {
        "sourcePath": "/var/run/docker.sock"
      }
    },
    {
      "name": "s3-conf",
      "host": {
        "sourcePath": "/var/app/data/conf"
      }
    },
    {
      "name": "s3-license",
      "host": {
        "sourcePath": "/var/app/data/license"
      }
    },
    {
      "name": "s3-cert",
      "host": {
        "sourcePath": "/var/app/data/cert"
      }
    },
    {
      "name": "s3-plugins",
      "host": {
        "sourcePath": "/var/app/data/plugins"
      }
    },
    {
      "name": "local-data",
      "host": {
        "sourcePath": "/var/app/data/data"
      }
    },
    {
      "name": "env-file",
      "host": {
        "sourcePath": "/etc/env"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "app",
      "image": "098786959887.dkr.ecr.us-west-2.amazonaws.com/flo-hivemq:latest",
      "essential": true,
      "memoryReservation": 512,
      "ulimits": [
        {
          "softLimit": 65536,
          "hardLimit": 65536,
          "name": "nofile"
        }
      ],
      "portMappings": [
        {
          "hostPort": 1883,
          "containerPort": 1883
        },
        {
          "hostPort": 8883,
          "containerPort": 8883
        },
        {
          "hostPort": 8884,
          "containerPort": 8884
        },
        {
          "hostPort": 8000,
          "containerPort": 8000
        },
        {
          "hostPort": 8001,
          "containerPort": 8001
        },
        {
          "hostPort": 9001,
          "containerPort": 9001
        },
        {
          "hostPort": 8778,
          "containerPort": 8778
        },
        {
          "hostPort": 8080,
          "containerPort": 8080
        },

        {
          "hostPort": 8081,
          "containerPort": 8081
        },
        {
          "hostPort": 9190,
          "containerPort": 9190
        },
        {
          "hostPort": 9191,
          "containerPort": 9191
        }

      ],
      "mountPoints": [
        {
          "sourceVolume": "local-docker",
          "containerPath": "/var/run/docker.sock"
        },
        {
          "sourceVolume": "awseb-logs-app",
          "containerPath": "/var/log/app"
        },
        {
          "sourceVolume": "s3-conf",
          "containerPath": "/app/conf"
        },
        {
          "sourceVolume": "s3-license",
          "containerPath": "/app/license"
        },
        {
          "sourceVolume": "s3-cert",
          "containerPath": "/app/cert"
        },
        {
          "sourceVolume": "local-data",
          "containerPath": "/app/data"
        },
        {
          "sourceVolume": "s3-plugins",
          "containerPath": "/app/external-plugins"
        },
        {
          "sourceVolume": "env-file",
          "containerPath": "/etc/env"
        }
      ]
    }
  ]
}