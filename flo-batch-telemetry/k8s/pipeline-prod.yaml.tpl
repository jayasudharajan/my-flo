image:
  tag: "${CI_PIPELINE_ID}"
secrets:
  datas:
    APPLICATION_NAME: "flo-batch-telemetry"
    ENVIRONMENT: "production"
    FLO_HTTP_PORT: "${FLO_HTTP_PORT_PROD}"
    FLO_KAFKA_CN: "${FLO_KAFKA_CN_PROD}"
    FLO_KAFKA_GROUP_ID: "${FLO_KAFKA_GROUP_ID_PROD}"
    FLO_SQS_NAME_BULK_FILE: "${FLO_SQS_NAME_BULK_FILE_PROD}"
    FLO_TELEMETRY_LATEST_TOPIC: "${FLO_TELEMETRY_LATEST_TOPIC_PROD}"
    FLO_REDIS_CN: "${FLO_REDIS_CN_PROD}"
    FLO_S3_BUCKET_TELEMETRY_APPEND: "flosecurecloud-telemetry-batch-append"
    FLO_S3_BUCKET_TELEMETRY_APPEND_HF: "${FLO_S3_BUCKET_TELEMETRY_APPEND_HF_PROD}"
    FLO_CONCAT_MAX_BATCH: "${FLO_CONCAT_MAX_BATCH_PROD}"
    FLO_CONCAT_MAX_WAIT: "50"
    FLO_CONCAT_RECOVER_MAX_PULL_BATCH: "${FLO_CONCAT_RECOVER_MAX_PULL_BATCH_PROD}"
    FLO_CONCAT_MOD_WITHIN_S: "5"
    FLO_CONCAT_BATCH_WITHIN_PCT: "50"
    FLO_S3_BULK_TELEMETRY_BUCKET: "flosecurecloud-bulk-device-telemetry"
    FLO_LOG_MIN_LEVEL: "1"
    FLO_CONCAT_MAX_ROWS: "${FLO_CONCAT_MAX_ROWS_PROD}"
    FLO_HTTP_LOG_PATH_IGNORES: "/|/ping"
    FLO_REBULK_KEY: "${FLO_REBULK_KEY_PROD}"
    FLO_REBULK_THREADS: "${FLO_REBULK_THREADS_PROD}"
    FLO_REBULK_PATH_KEY: "${FLO_REBULK_PATH_KEY_PROD}"
    FLO_REBULK_PATH_BATCH: "${FLO_REBULK_PATH_BATCH_PROD}"
    FLO_REBULK_PATH_THREADS: "${FLO_REBULK_PATH_THREADS_PROD}"
    FLO_REBULK_PATH_SLEEP_MS: "${FLO_REBULK_PATH_SLEEP_MS_PROD}"