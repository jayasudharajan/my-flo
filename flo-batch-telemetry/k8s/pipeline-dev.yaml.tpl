image:
  tag: "${CI_PIPELINE_ID}"
secrets:
  datas:
    APPLICATION_NAME: "flo-batch-telemetry"
    ENVIRONMENT: "development"
    FLO_HTTP_PORT: "${FLO_HTTP_PORT}"
    FLO_KAFKA_CN: "${FLO_KAFKA_CN}"
    FLO_KAFKA_GROUP_ID: "${FLO_KAFKA_GROUP_ID}"
    FLO_SQS_NAME_BULK_FILE: "${FLO_SQS_NAME_BULK_FILE}"
    FLO_TELEMETRY_LATEST_TOPIC: "${FLO_TELEMETRY_LATEST_TOPIC}"
    FLO_REDIS_CN: "${FLO_REDIS_CN}"
    FLO_S3_BUCKET_TELEMETRY_APPEND: "flocloud-batch-append"
    FLO_S3_BUCKET_TELEMETRY_APPEND_HF: "${FLO_S3_BUCKET_TELEMETRY_APPEND_HF}"
    FLO_CONCAT_MAX_BATCH: "5"
    FLO_CONCAT_MAX_WAIT: "3"
    FLO_CONCAT_RECOVER_MAX_PULL_BATCH: "5"
    FLO_CONCAT_MOD_WITHIN_S: "5"
    FLO_CONCAT_BATCH_WITHIN_PCT: "50"
    FLO_S3_BULK_TELEMETRY_BUCKET: "flocloud-bulk-device-telemetry"
    FLO_LOG_MIN_LEVEL: "1"
    FLO_CONCAT_MAX_ROWS: "${FLO_CONCAT_MAX_ROWS}"
    FLO_HTTP_LOG_PATH_IGNORES: "/|/ping"
    FLO_REBULK_KEY: "reBulk:q0"
    FLO_REBULK_THREADS: "4"