{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "ssmmessages:CreateControlChannel",
                "ssmmessages:CreateDataChannel",
                "ssmmessages:OpenControlChannel",
                "ssmmessages:OpenDataChannel"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "ssm:GetParameterHistory",
                "ssm:GetParametersByPath",
                "ssm:GetParameters",
                "ssm:GetParameter",
                "ssm:DeleteParameters"
            ],
            "Resource": [
                "arn:aws:ssm:us-west-2:260332691473:parameter/managed/terraform/lte-proxy/*",
                "arn:aws:ssm:us-west-2:260332691473:parameter/managed/terraform/lte-proxy/ssl/cert/multidomain_meetflo_com_ca_bundle",
                "arn:aws:ssm:us-west-2:260332691473:parameter/managed/terraform/lte-proxy/ssl/cert/multidomain_meetflo_com_cert",
                "arn:aws:ssm:us-west-2:260332691473:parameter/managed/terraform/lte-proxy/ssl/cert/multidomain_meetflo_com_key"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetEncryptionConfiguration"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "kms:Decrypt"
            ],
            "Resource": [
                "arn:aws:kms:us-west-2:260332691473:key/00cc519e-74f7-47d6-bf81-c339812660cc"
            ]
        }
    ]
}
