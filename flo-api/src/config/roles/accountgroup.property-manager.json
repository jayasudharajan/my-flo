{
  "roles": "AccountGroup.*.property-manager",
  "allows": [
    {
      "resources": ["AccountGroup"],
      "permissions": [
        "retrieveAllGroupICDs",
        "retrieveICD",
        "retrievePendingNotifications",
        "impersonate",
        "retrieveThisWeekMeasurements",
        "retrieveDailyAlertCountByGroupId",
        "open-valve",
        "close-valve"
      ]
    },
    {
      "resources": ["Client:property-sentinel"],
      "permissions": [
        "login"
      ]
    }
  ]
}