    {
      "resources": ["ICDAlarmLog"],
      "permissions": [
        "create"
      ]
    },
    {
      "resources": ["ICDAlarmNotificationStatusRegistry"],
      "permissions": [
        "create"
      ]
    },
    {
      "resources": ["Onboarding"],
      "permissions": [
        "triggerEvent"
      ]
    },
    {
      "resources": ["AccountSubscription"],
      "permissions": [
        "retrieveByUserId",
        "retrieve",
        "retrieveSubscriptionPlan"
      ]
    },
    {
      "resources": ["WeeklyReport"],
      "permissions": [
        "issueUnsubscribeLink"
      ]
    },
    {
      "resources": ["Directives"],
      "permissions": [
        "close-valve"
      ]
  }
  ]
}
