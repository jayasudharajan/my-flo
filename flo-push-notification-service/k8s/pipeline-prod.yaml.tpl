image:
  tag: "${CI_PIPELINE_ID}"
secrets:
  datas:
    ENVIRONMENT: production
    ANDROID_SNS_ARN: "${ANDROID_SNS_ARN_PROD}"
    APPLICATION_NAME: "flo-push-notification-service"
    FLO_API_CLIENT_ID: "${FLO_API_CLIENT_ID_PROD}"
    FLO_API_CLIENT_SECRET: "${FLO_API_CLIENT_SECRET_PROD}"
