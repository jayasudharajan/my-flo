    IOS_SNS_ARN: "${IOS_SNS_ARN_PROD}"
    JMX_ENABLED: "${JMX_ENABLED_PROD}"
    KAFKA_HOSTS: "${KAFKA_HOSTS_PROD}"
    LOG_LEVEL: "${LOG_LEVEL_PROD}"
    PUSH_NOTIFICATION_CONSUMER_GROUP_ID: "${PUSH_NOTIFICATION_CONSUMER_GROUP_ID_PROD}"
    PUSH_NOTIFICATION_CONSUMER_POLL_TIMEOUT: "${PUSH_NOTIFICATION_CONSUMER_POLL_TIMEOUT_PROD}"
