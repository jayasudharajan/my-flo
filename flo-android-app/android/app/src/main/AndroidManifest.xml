            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="floapp" android:host="register2" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="floapp" android:host="register2" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.flotechnologies.intent.action.INCIDENT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
